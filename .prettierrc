{
    "tabWidth": 4,
    "semi": true,
    "arrowParens": "always",
    "trailingComma": "all",
    "useTabs": false,
    "singleQuote": false,
    "printWidth": 100,
    "overrides": [
        {
            "files": "*.ts",
            "options": { "parser": "typescript" }
        },
        {
            "files": ".prettierrc",
            "options": { "parser": "json" }
        },
        {
            "files": ".json",
            "options": { "parser": "json" }
        }
    ]
}
